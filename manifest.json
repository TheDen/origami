{
  "manifest_version": 3,
  "name": "Origami: ğ• (Twitter) Adblock",
  "version": "1.0",
  "description": "Block ğ• (Twitter) Ads",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["http://wwww.twitter.com/*", "https://twitter.com/*"],
      "run_at": "document_end"
    }
  ]
}
